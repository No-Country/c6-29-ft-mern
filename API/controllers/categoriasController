const categorias = require('../modelos/categorias');

module.exports ={
create: async function(req,res,next){
        try {
        const categoria = new categorias({
            nombre:req.body.nombre
        })  
        const document = await categoria.save()
        res.status(201).json(document)  
      } catch (error) {
          console.log(error);
        }
      }
    }